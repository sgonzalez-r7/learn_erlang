%% Description TODO: edit
%%
%% Parameters:
%% Email - a raw parent email, which has the following data structure:
%%         {Type, SubType, Headers, Parameters, Body}, see
%%         https://github.com/Vagabond/gen_smtp/blob/master/src/mimemail.erl
%%         for details of the API.


attached_email(Parts) ->
  Attached_email = find_part({<<"message">>, <<"rfc822">>}, Parts),
  case Part of
    none -> none;
    _ -> Attached_email
  end


parse(Email) ->
% assume


parse_raw(Raw_mail) ->
  {Type, SubType, Headers, Parameters, Body} = Parent_email = mimemail:decode(Raw_email),

  case Body of
    Parts when is_list(Parts) ->
      attached_email(Parts)
    _ ->
      Parent_email
  end,


  {_, _, Parent_headers, _, Parent_body} = Parent_email,
  _Submitter = proplists:get_value(<<"From">>, Parent_headers)



  case Parent_email of
    Parts when is_list(Parts) ->
      {_, _, Headers, _, _} = Attached_email,
              Date = proplists:get_value(<<"Date">>,    Headers),
              From = proplists:get_value(<<"From">>,    Headers),
      Senders_name = senders_name(From),
           Subject = proplists:get_value(<<"Subject">>, Headers),
      #{
                date => Date,
        senders_name => Senders_name,
             subject => Subject,
           submitter => <<"submitter">>
      };
    _Else ->
      none
    end.

find_part({Type, SubType}, Parts) ->
  Found = fun({T, ST, _H, _P, _B}) -> T =:= Type andalso ST =:= SubType end,
  Found_parts = lists:filter(Found, Parts),
  case Found_parts ->
    [] ->
      none;
    _ ->
      hd(Found_parts)
  end.

senders_name(From) ->
        String = binary:bin_to_list(From),
        Tokens = string:tokens(String, " "),
          Name = fun(X) ->  string:str(X, "<") =:= 0 end, % not email, e.g. <a@b.com>
   Name_tokens = lists:filter(Name, Tokens),
  Senders_name = string:join(Name_tokens, " "),
  list_to_binary(Senders_name).


